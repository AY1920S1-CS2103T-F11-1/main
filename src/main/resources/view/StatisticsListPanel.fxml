<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="555.0" prefWidth="520.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <GridPane prefHeight="555.0" prefWidth="520.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <HBox prefHeight="134.0" prefWidth="520.0" GridPane.columnSpan="2">
                    <children>
                        <VBox prefHeight="185.0" prefWidth="172.0"
                              style="-fx-background-color: #eaf0f4; -fx-background-radius: 10px;">
                            <children>
                                <Label alignment="TOP_CENTER" prefHeight="36.0" prefWidth="172.0" text="Total"
                                       textAlignment="CENTER">
                                    <font>
                                        <Font name="Arial" size="24.0"/>
                                    </font>
                                </Label>
                                <Label alignment="TOP_CENTER" prefHeight="36.0" prefWidth="172.0" text="Teams"
                                       textAlignment="CENTER">
                                    <font>
                                        <Font name="Arial" size="24.0"/>
                                    </font>
                                </Label>
                                <Label fx:id="totalTeamsLabel" alignment="CENTER" prefHeight="116.0" prefWidth="172.0"
                                       text="Label">
                                    <font>
                                        <Font size="36.0"/>
                                    </font>
                                </Label>
                            </children>
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                        </VBox>
                        <VBox prefHeight="185.0" prefWidth="346.0">
                            <children>
                                <BarChart fx:id="teamBarChart"
                                          style="-fx-background-color: #d9e3ec; -fx-background-radius: 10px;"
                                          title="Team Distribution(By Subject)">
                                    <xAxis>
                                        <CategoryAxis fx:id="subjectAxis" side="BOTTOM"/>
                                    </xAxis>
                                    <yAxis>
                                        <NumberAxis fx:id="teamYAxis" side="LEFT"/>
                                    </yAxis>
                                </BarChart>
                            </children>
                        </VBox>
                    </children>
                </HBox>
                <HBox prefHeight="134.0" prefWidth="520.0" GridPane.columnSpan="2" GridPane.rowIndex="1">
                    <children>
                        <VBox prefHeight="185.0" prefWidth="172.0"
                              style="-fx-background-color: #d9e3ec; -fx-background-radius: 10px;">
                            <children>
                                <Label alignment="TOP_CENTER" prefHeight="36.0" prefWidth="172.0" text="Total"
                                       textAlignment="CENTER">
                                    <font>
                                        <Font name="Arial" size="24.0"/>
                                    </font>
                                </Label>
                                <Label alignment="TOP_CENTER" prefHeight="36.0" prefWidth="172.0" text="Mentors"
                                       textAlignment="CENTER">
                                    <font>
                                        <Font name="Arial" size="24.0"/>
                                    </font>
                                </Label>
                                <Label fx:id="totalMentorsLabel" alignment="CENTER" prefHeight="115.0" prefWidth="172.0"
                                       text="Label" textAlignment="CENTER">
                                    <font>
                                        <Font size="36.0"/>
                                    </font>
                                </Label>
                            </children>
                        </VBox>
                        <VBox prefHeight="185.0" prefWidth="346.0"
                              style="-fx-background-color: #c8d7e3#c8d7e3; -fx-background-radius: 10px;">
                            <children>
                                <BarChart fx:id="mentorBarChart" title="Mentor Distribution(By Specialisation)">
                                    <xAxis>
                                        <CategoryAxis fx:id="specialisationAxis" side="BOTTOM"/>
                                    </xAxis>
                                    <yAxis>
                                        <NumberAxis fx:id="mentorYAxis" side="LEFT"/>
                                    </yAxis>
                                </BarChart>
                            </children>
                        </VBox>
                    </children>
                </HBox>
                <HBox prefHeight="134.0" prefWidth="520.0" GridPane.columnSpan="2" GridPane.rowIndex="2">
                    <children>
                        <VBox prefHeight="185.0" prefWidth="172.0"
                              style="-fx-background-color: #eaf0f4; -fx-background-radius: 10px;">
                            <children>
                                <Label alignment="TOP_CENTER" prefHeight="36.0" prefWidth="172.0" text="Total"
                                       textAlignment="CENTER">
                                    <font>
                                        <Font name="Arial" size="24.0"/>
                                    </font>
                                </Label>
                                <Label alignment="TOP_CENTER" prefHeight="36.0" prefWidth="172.0" text="Participants"
                                       textAlignment="CENTER">
                                    <font>
                                        <Font name="Arial" size="24.0"/>
                                    </font>
                                </Label>
                                <Label fx:id="totalParticipantsLabel" alignment="CENTER" prefHeight="109.0"
                                       prefWidth="172.0" text="Label">
                                    <font>
                                        <Font name="Arial" size="36.0"/>
                                    </font>
                                </Label>
                            </children>
                        </VBox>
                        <VBox prefHeight="185.0" prefWidth="172.0"/>
                    </children>
                </HBox>
            </children>
        </GridPane>
    </children>
</AnchorPane>
